# Auto detect text files and perform LF normalization
* text=auto
*.psd filter=lfs diff=lfs merge=lfs -text
*.blend filter=lfs diff=lfs merge=lfs -text
*.spp filter=lfs diff=lfs merge=lfs -text
*.wwu filter=lfs diff=lfs merge=lfs -text
*.validationcache filter=lfs diff=lfs merge=lfs -text
*.wsettings filter=lfs diff=lfs merge=lfs -text
*.wproj filter=lfs diff=lfs merge=lfs -text